function test() {
   var arr = [1,2,3,4];
   var target = 1;
   for (var i = 0; i < arr.length; i ++) {
       for(var j = 0;j < arr.length; j++) {
           if (arr[i]-arr[j] == 1 ) {
               console.log("(",arr[i],",",arr[j],")");
           }
       }
       
   }
}
test();
